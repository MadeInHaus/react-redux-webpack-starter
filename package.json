{
    "name": "react-redux-webpack-starter",
    "version": "0.0.1",
    "engines": {
        "node": ">=6.0.0"
    },
    "description": "React-Redux-Webpack starter with common tasks and scenarios",
    "scripts": {
        "build": "NODE_ENV=production webpack --config ./config/webpack.prod.js --progress && npm run build:server",
        "build:dev": "webpack --config ./config/webpack.dev.js --progress && npm run build:server",
        "build:server": "webpack --config ./config/webpack.server.js --progress",
        "clean:build": "rimraf build",
        "prebuild": "npm run clean:build",
        "prestart": "npm run clean:build",
        "start": "npm run build:dev && nodemon ./server-dev.js",
        "format": "prettier-eslint --single-quote --trailing-comma es5 --tab-width 4 --prettierLast --write \"src/**/*.{js,jsx}\" \"gulp/**/*.js\""
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/MadeInHaus/react-redux-webpack-starter.git"
    },
    "license": "MIT",
    "devDependencies": {
        "babel-cli": "6.24.1",
        "babel-core": "6.25.0",
        "babel-eslint": "7.2.3",
        "babel-loader": "7.1.1",
        "babel-plugin-module-resolver": "2.7.1",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-plugin-transform-decorators-legacy": "1.3.4",
        "babel-plugin-transform-flow-strip-types": "6.22.0",
        "babel-plugin-transform-react-constant-elements": "6.23.0",
        "babel-plugin-transform-react-remove-prop-types": "0.4.8",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-node6": "11.0.0",
        "babel-preset-react": "6.24.1",
        "babel-preset-stage-0": "6.24.1",
        "copy-webpack-plugin": "4.0.1",
        "css-loader": "^0.28.4",
        "eslint": "4.4.1",
        "eslint-plugin-babel": "4.1.2",
        "eslint-plugin-react": "7.2.0",
        "extend": "3.0.1",
        "extract-text-webpack-plugin": "3.0.0",
        "file-loader": "0.11.2",
        "http-proxy": "1.16.2",
        "node-sass": "4.5.3",
        "nodemon": "1.11.0",
        "postcss-loader": "2.0.6",
        "prettier": "1.5.3",
        "prettier-eslint": "6.4.2",
        "prettier-eslint-cli": "4.1.1",
        "raw-loader": "0.5.1",
        "react-hot-loader": "^1.3.1",
        "rimraf": "2.6.1",
        "sass-loader": "6.0.6",
        "sass-variable-loader": "0.1.2",
        "style-loader": "^0.18.2",
        "uglifyjs-webpack-plugin": "^0.4.6",
        "url-loader": "0.5.9",
        "webpack": "3.5.2",
        "webpack-dev-middleware": "1.12.0",
        "webpack-hot-middleware": "2.18.2",
        "webpack-merge": "4.1.0",
        "webpack-node-externals": "1.6.0"
    },
    "dependencies": {
        "axios": "^0.16.2",
        "classnames": "2.2.5",
        "compression": "1.7.0",
        "connect-inject": "0.4.0",
        "debug": "3.0.0",
        "dotenv": "4.0.0",
        "express": "4.15.4",
        "imagemin-webpack-plugin": "^1.5.0-beta.0",
        "immutable": "^3.8.1",
        "keymirror": "^0.1.1",
        "lodash": "4.17.4",
        "prop-types": "15.5.10",
        "react": "15.6.1",
        "react-dom": "15.6.1",
        "react-redux": "5.0.6",
        "react-router": "4.1.2",
        "react-router-dom": "4.1.2",
        "redux": "3.7.2",
        "redux-thunk": "^2.2.0",
        "serialize-javascript": "^1.4.0"
    }
}
